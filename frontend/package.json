        "react": "^18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "^18.2.0",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.0",
        "vite-jsconfig-paths": "^2.0.1",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@types/react": "^18.2.55",
